{
  "name": "drought.ca.gov",
  "version": "1.1.0",
  "description": "",
  "main": "index.html",
  "scripts": {
    "build": "eleventy",
    "build:dev": "cross-env NODE_ENV=development eleventy",
    "watch": "eleventy --serve",
    "watch:dev": "cross-env NODE_ENV=development eleventy --serve --incremental",
    "start": "npm run watch",
    "dev": "npm run watch:dev",
    "test": "npx playwright test",
    "test:headed": "npx playwright test --headed",
    "test:cypress:tool": "npx cypress open",
    "test:cypress": "npm-run-all test:server:background test:cypress:tool",
    "test:playwright": "npm-run-all test:server:background test",
    "test:playwright:headed": "npm-run-all test:server:background test:headed",
    "test:server": "npx web-dev-server --root-dir docs/",
    "test:server:background": "npx web-dev-server --root-dir docs/ &",
    "test:setup": "npx playwright install-deps chromium && npx playwright install",
    "git:merge:dev-into-main": "./src/sh/git-merge-development-into-main.sh",
    "git:merge:main-into-dev": "./src/sh/git-merge-main-into-development.sh"
  },
  "keywords": [
    "ca.gov",
    "@cagov",
    "drought.ca.gov"
  ],
  "author": "Office of Digital Innovation",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.14.1",
    "@web/dev-server": "^0.1.22"
  },
  "dependencies": {
    "@11ty/eleventy": "^1.0.0-beta.3",
    "@cagov/11ty-build-system": "^0.1.3",
    "@cagov/ds-accordion": "^1.0.8",
    "@cagov/ds-agency-footer": "^1.0.2",
    "@cagov/ds-back-to-top": "^1.0.2",
    "@cagov/ds-base-css": "^1.0.11",
    "@cagov/ds-branding": "^1.0.8",
    "@cagov/ds-button-grid": "^2.0.1",
    "@cagov/ds-content-navigation": "^1.0.7",
    "@cagov/ds-dropdown-menu": "^1.0.15",
    "@cagov/ds-external-link-icon": "^1.0.3",
    "@cagov/ds-feature-card": "^1.0.5",
    "@cagov/ds-feedback": "^1.0.11",
    "@cagov/ds-minus": "^1.0.0",
    "@cagov/ds-pagination": "^1.0.5",
    "@cagov/ds-pdf-icon": "^1.0.2",
    "@cagov/ds-plus": "^1.0.0",
    "@cagov/ds-skip-to-content": "^1.0.1",
    "@cagov/ds-statewide-footer": "^1.0.5",
    "@cagov/ds-statewide-header": "^1.0.11",
    "@cagov/ds-step-list": "^1.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "cheerio": "^1.0.0-rc.10",
    "clean-css": "^5.1.2",
    "cross-env": "^7.0.3",
    "html-minifier": "^4.0.0",
    "node-fetch": "^2.6.1",
    "rollup-plugin-terser": "^7.0.2"
  }
}
