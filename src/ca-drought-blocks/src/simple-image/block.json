{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "ca-drought-blocks/simple-image",
  "version": "0.1.0",
  "title": "Simple Image",
  "category": "ca-drought-blocks",
  "icon": "dashboard",
  "description": "Inserts a simple image, without typical figure-related mark-up added by Wordpress.",
  "attributes": {
    "imgId": {
      "type": "number",
      "default": 0
    }, 
    "imgUrl": {
      "type": "string",
      "default": ""
    },
    "imgAlt": {
      "type": "string",
      "default": ""
    }
  },
  "supports": {
    "html": false
  },
  "textdomain": "simple-image",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}
